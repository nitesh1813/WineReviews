    "def getdocvector(sentence):\n",
    "    sentence=sentence.split()\n",
    "#     print(sentence)\n",
    "    vec=np.zeros((1,50))\n",
    "    total=0\n",
    "    for word in sentence:\n",
    "        try:\n",
    "            vec+=wine2vec[word].reshape(1,50)\n",
    "            total+=1\n",
    "        except:pass\n",
    "        \n",
    "#     print(vec)\n",
    "    if total!=0:\n",
    "        vec=vec*1.0/total\n",
    "    return vec[0]"
   ]
